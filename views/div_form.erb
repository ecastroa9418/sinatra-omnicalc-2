<h1>New division</h1>

<form action="/wizard_divide">
  <label for="dividend">
    Divide this:
  </label>
  <input id="dividend" name="first_number" text="text">

  <label for="divisor">
    by this:
  </label>
  <input id="divisor" name="second_number" text="text">

  <button>
    Divide!
  </button>
</form>
